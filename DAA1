// Program 1a : Selection Sort 

#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[100];
    for(int i =0;i<n;i++) scanf("%d",&arr[i]);
    int swapCnt = 0, cmpCnt = 0;
    for(int i =0;i<n-1;i++){
        int minIdx = i;
        for(int j = i+1;j<n;j++){
            cmpCnt++;
            if(arr[j] < arr[minIdx]) minIdx = j;
        }
        if(minIdx != i){
            int temp = arr[i];
            arr[i] = arr[minIdx];
            arr[minIdx] = temp;
            swapCnt++;
        }
    }
    for(int i = 0;i<n;i++) printf("%d ", arr[i]);
    printf("\n%d %d\n", swapCnt, cmpCnt);
    
    return 0;
}

// Program 1b: Bubble Sort

#include<stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    int arr[100];
    for(int i = 0;i<n;i++) scanf("%d", &arr[i]);
    int swapCnt = 0, cmpCnt = 0;
    for(int i = 0;i<n-1;i++){
        int swapped = 0;
        for(int j = 0;j<n-i-1;j++){// largest element goes to last idx
            cmpCnt++;
            if(arr[j]> arr[j+1]){
                int temp = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp;
                swapCnt++;
                swapped = 1;
            }
        }
        if(!swapped) break;
    }
    for(int i = 0;i<n;i++) printf("%d ",arr[i]);
    printf("\n%d %d\n", swapCnt, cmpCnt);
    return 0;
}
